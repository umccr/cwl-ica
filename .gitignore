# Items in expressions / tools and workflows that don't end in .cwl
expressions/*/*/*
!expressions/*/*/*.cwl
!expressions/*/*/typescript-expressions/
expressions/*/*/typescript-expressions/.yarn/
expressions/*/*/typescript-expressions/.yarnrc.yml
expressions/*/*/typescript-expressions/.pnp*

tools/*/*/*
!tools/*/*/*.cwl
!tools/*/*/typescript-expressions/
tools/*/*/typescript-expressions/.yarn/
tools/*/*/typescript-expressions/.yarnrc.yml
tools/*/*/typescript-expressions/.pnp*

workflows/*/*/*
!workflows/*/*/*.cwl
!workflows/*/*/typescript-expressions/
workflows/*/*/typescript-expressions/.yarn/
workflows/*/*/typescript-expressions/.yarnrc.yml
workflows/*/*/typescript-expressions/.pnp*

#### Items in schemas that don't end in .yaml
schemas/*/*/*
!schemas/*/*/*.yaml
!schemas/*/*/*.ts

#### Items in config that don't end in .yaml
config/*/*/*
!config/*/*/*.yaml

#### Caches in typescript expressions
typescript-expressions/*/*/.yarn/
